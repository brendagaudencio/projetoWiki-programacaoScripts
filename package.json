{
  "name": "projetowiki-programacaoscripts",
  "version": "2.0.0",
  "description": "Wiki sobre degustação de vinhos - Versão Fullstack para disciplina Programação de Scripts",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:watch": "ts-node --watch src/server.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "keywords": [
    "vinhos",
    "degustacao",
    "wiki",
    "fatec",
    "programacao-scripts"
  ],
  "authors": [
    "Brenda Gaudêncio <brendagaudencio@fatec.sp.gov.br>",
    "Marcos Moreira <marcos.moreira@fatec.sp.gov.br>"
  ],
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "method-override": "^3.0.0",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/connect-flash": "^0.0.40",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/express-validator": "^2.20.33",
    "@types/node": "^24.0.10",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brendagaudencio/projetoWiki-programacaoScripts"
  },
  "bugs": {
    "url": "https://github.com/brendagaudencio/projetoWiki-programacaoScripts/issues"
  },
  "homepage": "https://github.com/brendagaudencio/projetoWiki-programacaoScripts#readme"
}
